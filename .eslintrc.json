{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "project": "./tsconfig.json"
  },
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "plugins": ["@typescript-eslint"],
  "env": {
    "node": true,
    "commonjs": true,
    "es2021": true
  },
  "rules": {
    "@typescript-eslint/no-var-requires": "off"
  },
  "ignorePatterns": []
}
